{
  "compilerOptions": {
		"composite": true,
		"experimentalDecorators": true,
		"isolatedModules": true,
		"lib": ["esnext"],
		"module": "esnext",
		"moduleResolution": "node",
		"noEmit": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "esnext",
		"jsx": "react-jsx",
		"types": ["@docusaurus/module-type-aliases", "node", "vite/client"],
	},
}
